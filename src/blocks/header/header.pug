mixin header(buttons, oneLi, twoLi, threeLi, fourLi, fifeLi, sixLi, buttonOneName, buttonTwoName, buttonStyleOne, buttonStyleTwo, widthNav)
    .containerHeader
        include ../toxin/toxin
        +toxin()
        .containerNav(class=widthNav)
            nav 
                ul
                    li.bodyText.text_height_17.text_weight_bold=oneLi
                    li.bodyText.text_height_17=twoLi
                    li.bodyText.text_height_17=threeLi 
                        i.fa.fa-angle-down.fa-lg.text_color_50(aria-hidden="true")
                    li.bodyText.text_height_17=fourLi
                    li.bodyText.text_height_17=fifeLi
                    li.bodyText.text_height_17=sixLi
            if (!buttons)
                p.bodyText.text_height_17 Cosmin Negoita
            if (buttons)
                .buttonsContainer
                    include ../buttons/buttons
                    +button(buttonOneName,'button_border_withBorder', 'button_size_mini', buttonStyleOne)
                    +button(buttonTwoName, 'button_color_gradient', 'button_size_mini', buttonStyleTwo)
    