mixin slider(name)
    .sliderRow.bodyText
        h3.text.text_type_h3=name
        .sliderPrice.text.ext_type_h3.text_weight_normal.text_color_50
            .output.o0 
            p.sliderHyphen - 
            .output.o1 
    .containerSlider
        .slider
            .track
        .thumb.t0 
        .thumb.t1